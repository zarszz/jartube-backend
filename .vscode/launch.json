{
    "version": "0.2.0",
    "configurations": [
    {
        "type": "node",
        "request": "launch",
        "name": "nodemon",
        "runtimeExecutable": "${workspaceFolder}/node_modules/nodemon/bin/nodemon.js",
        "program": "${workspaceFolder}/src/server.ts",
        "restart": true,
        "internalConsoleOptions": "neverOpen",
        "skipFiles": [
            "<node_internals>/**"
        ],
    }]
}